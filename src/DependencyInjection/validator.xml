<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Kellerkinder\TwigCsFixer\Validator\PipePrefixSpacingValidator">
            <tag name="twig_cs_fixer.validator" />
        </service>

        <service id="Kellerkinder\TwigCsFixer\Validator\PipeSuffixSpacingValidator">
            <tag name="twig_cs_fixer.validator" />
        </service>

        <service id="Kellerkinder\TwigCsFixer\Validator\SpaceLineValidator">
            <tag name="twig_cs_fixer.validator" />
        </service>

        <service id="Kellerkinder\TwigCsFixer\Validator\TrailingSpaceValidator">
            <tag name="twig_cs_fixer.validator" />
        </service>

        <service id="Kellerkinder\TwigCsFixer\Validator\UppercaseVariableValidator">
            <tag name="twig_cs_fixer.validator" />
        </service>
    </services>
</container>
